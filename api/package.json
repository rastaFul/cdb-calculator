{
    "name": "cdb-calculator-api",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.js",
        "start:dev": "nodemon src/index.js",
        "generate": "node src/cli.js",
        "lint": "eslint . --ext .js",
        "lint:fix": "eslint . --ext .js --fix",
        "test": "jest --detectOpenHandles --forceExit"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "npm run lint:fix",
            "npm test",
            "git add"
        ]
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ]
    },
    "keywords": ["cdb", "calculator"],
    "author": "Rodrigo Nascimento",
    "license": "MIT",
    "dependencies": {
        "csv-parser": "^3.0.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "moment-timezone": "^0.5.33",
        "mongoose": "^5.11.15"
    },
    "devDependencies": {
        "@shelf/jest-mongodb": "^1.2.3",
        "eslint": "^7.6.0",
        "husky": "^4.2.5",
        "jest": "^26.4.0",
        "lint-staged": "^10.2.11",
        "nodemon": "^2.0.4"
    }
}
